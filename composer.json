{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "barryvdh/laravel-debugbar": "^3.0",
        "creativeorange/gravatar": "~1.0",
        "doctrine/dbal": "^2.5",
        "fideloper/proxy": "^4.0",
        "google/recaptcha": "^1.1",
        "intervention/image": "^2.4",
        "jelovac/bitly4laravel": "3.*",
        "jeremykenedy/laravel-exception-notifier": "^1.1",
        "jeremykenedy/laravel-roles": "^1.2",
        "jrm2k6/cloudder": "^0.6.0",
        "lab404/laravel-impersonate": "^1.2",
        "laravel/cashier-braintree": "~2.0",
        "laravel/framework": "5.6.*",
        "laravel/horizon": "^1.3",
        "laravel/socialite": "^3.0",
        "laravel/tinker": "^1.0",
        "laravelcollective/html": "^5.5",
        "predis/predis": "^1.1",
        "rap2hpoutre/laravel-log-viewer": "^0.10.4",
        "socialiteproviders/37signals": "^3.0",
        "socialiteproviders/generators": "^2.0",
        "socialiteproviders/instagram": "^3.0",
        "socialiteproviders/twitch": "^3.0",
        "socialiteproviders/youtube": "^3.0",
        "spatie/laravel-cookie-consent": "^2.4",
        "thujohn/twitter": "^2.2",
        "webpatser/laravel-uuid": "^2.0",
        "willvincent/feeds": "^1.1"
    },
    "require-dev": {
        "beyondcode/laravel-er-diagram-generator": "^1.1",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
